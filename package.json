{
  "name": "rs-manager",
  "description": "Manage Rocksmith playlists, songs, stats and dlc's",
  "author": "SandiC",
  "version": "0.9.2",
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "aes-js": "^3.1.1",
    "album-art": "^2.0.1",
    "autoprefixer": "9.1.5",
    "await-spawn": "^2.1.1",
    "binary-parser": "^1.3.2",
    "chalk": "2.4.1",
    "chart.js": "^2.7.2",
    "chartjs-plugin-zoom": "^0.6.5",
    "csv-parse": "^3.1.2",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "electron-is-dev": "^1.0.1",
    "electron-prompt": "^1.2.0",
    "electron-window-state": "^5.0.1",
    "express": "^4.16.3",
    "find-process": "^1.1.3",
    "fs-extra": "7.0.0",
    "line-by-line": "^0.1.6",
    "moment": "^2.22.2",
    "node-abi": "^2.4.5",
    "object-assign": "4.1.1",
    "promise": "8.0.2",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-animated-number": "^0.4.4",
    "react-bootstrap": "^0.32.4",
    "react-bootstrap-table-next": "^1.2.0",
    "react-bootstrap-table2-filter": "^1.0.0",
    "react-bootstrap-table2-paginator": "^1.0.2",
    "react-chartkick": "^0.3.0",
    "react-collapsible": "^2.3.1",
    "react-datepicker": "^1.6.0",
    "react-dom": "^16.5.2",
    "react-moment": "^0.8.1",
    "react-tooltip": "^3.8.4",
    "request": "^2.34",
    "request-promise": "^4.2.2",
    "resolve": "1.8.1",
    "shortid": "^2.2.13",
    "sqlite": "^3.0.0",
    "sudo-prompt": "^8.2.3",
    "tough-cookie": "^2.4.3",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "electron": "electron .",
    "electron-start": "node src/electron-wait-react",
    "react-start": "node scripts/start.js",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "postinstall": "electron-rebuild  -f -w  sqlite3 && patch-package",
    "package-mac": "node scripts/package.js mac",
    "package-win": "node scripts/package.js win",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "1.0.0",
    "devtron": "^1.4.0",
    "electron": "^3.0.2",
    "electron-builder": "^20.28.4",
    "electron-packager": "^12.2.0",
    "electron-rebuild": "^1.8.2",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^3.0.3",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "foreman": "^3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "23.6.0",
    "patch-package": "^5.1.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "3.0.0",
    "react-dev-utils": "^5.0.1",
    "style-loader": "0.23.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "1.1.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "build": {
    "appId": "com.sandi.rsmanager",
    "win": {
      "icon": "./src/assets/icon.jpg"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": "github:sandiz/rs-manager"
}
