{
  "name": "rs-manager",
  "description": "Manage Rocksmith playlists, songs, stats and dlc's",
  "author": "sandiz",
  "version": "2.7.5",
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "about-window": "^1.12.1",
    "aes-js": "^3.1.1",
    "album-art": "^2.0.1",
    "await-spawn": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "binary-parser": "^1.3.2",
    "core-js": "^3.2.1",
    "css-toggle-switch": "^4.1.0",
    "csv-parse": "^4.4.6",
    "dotenv": "8.1.0",
    "dotenv-expand": "5.1.0",
    "electron-is-dev": "^1.0.1",
    "electron-prompt": "^1.2.0",
    "electron-window-state": "^5.0.1",
    "express": "^4.16.3",
    "find-process": "^1.1.3",
    "highcharts": "^7.1.3",
    "highcharts-react-official": "^2.0.0",
    "i18next": "^15.1.3",
    "i18next-node-fs-backend": "^2.1.3",
    "line-by-line": "^0.1.6",
    "marked": "^0.7.0",
    "memorystream": "^0.3.1",
    "merge": "^1.2.1",
    "moment": "^2.22.2",
    "naudiodon": "^2.1.0",
    "node-abi": "^2.7.0",
    "node-vdf": "^0.0.3",
    "object-assign": "4.1.1",
    "openid": "^2.0.6",
    "pidusage": "^2.0.17",
    "promise": "8.0.3",
    "prop-types": "^15.6.2",
    "psarcjs": "^0.1.4",
    "raf": "3.4.1",
    "react": "^16.4.1",
    "react-animated-number": "^0.4.4",
    "react-bootstrap-table-next": "^3.1.9",
    "react-bootstrap-table2-filter": "^1.0.0",
    "react-bootstrap-table2-paginator": "^2.0.8",
    "react-collapsible": "^2.3.1",
    "react-datepicker": "^1.8.0",
    "react-dom": "^16.5.2",
    "react-i18next": "^9.0.10",
    "react-select": "3.0.8",
    "react-sortable-hoc": "^1.10.1",
    "react-toastify": "^5.3.2",
    "react-tooltip": "^3.11.0",
    "registry-js": "^1.4.0",
    "request": "^2.34",
    "request-promise": "^4.2.2",
    "shell-env": "^3.0.0",
    "shortid": "^2.2.13",
    "sqlite": "^3.0.0",
    "steam-webapi": "^0.6.5",
    "sudo-prompt": "^9.0.0",
    "sweetalert2": "^8.16.3",
    "tmp": "^0.1.0",
    "wav": "^1.0.2",
    "wavefile": "^8.4.4",
    "whatwg-fetch": "^3.0.0",
    "worker-loader": "^2.0.0"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "electron": "electron .",
    "electron-start": "node src/electron-wait-react",
    "react-start": "react-app-rewired start",
    "pack": "build --dir",
    "dist-mac": "electron-builder install-app-deps && yarn run build && yarn run package-mac && yarn run dmg-mac",
    "dist-win": "electron-builder install-app-deps && yarn run build && yarn run package-win && yarn run exe-win",
    "postinstall": "electron-builder install-app-deps && patch-package",
    "package-mac": "node scripts/package.js mac",
    "package-win": "node scripts/package.js win",
    "dmg-mac": "yarn run electron-builder build --publish=never --mac -c.extraMetadata.main=src/electron-starter.js -c.mac.identity=null --prepackaged 'release-builds/Rocksmith Manager-darwin-x64'",
    "exe-win": "yarn run electron-builder build --publish=never --windows --c.extraMetadata.main=src/electron-starter.js --prepackaged \".\\release-builds\\Rocksmith Manager-win32-x64\"",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "contributors:check": "all-contributors check"
  },
  "devDependencies": {
    "all-contributors-cli": "^6.8.1",
    "babel-preset-react-app": "^9.0.1",
    "cross-spawn": "^6.0.5",
    "customize-cra": "^0.5.0",
    "electron": "^6.0.6",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.0.5",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "foreman": "^3.0.1",
    "google-translate": "^2.2.0",
    "jest": "24.9.0",
    "patch-package": "^6.1.2",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "^3.1.1",
    "webpack-dev-server": "^3.8.0"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "build": {
    "productName": "Rocksmith Manager",
    "appId": "com.sandi.rsmanager",
    "files": [
      "src/",
      ".",
      "!dev${/*}",
      "!samples${/*}",
      "!demos${/*}",
      "!doc${/*}",
      "!test${/*}",
      "!functest${/*}",
      "!patches/*",
      "!packages/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "icon": "src/assets/icons/mac/mac.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "electronLanguages": [
        "en"
      ]
    },
    "win": {
      "icon": "src/assets/icons/win/rs.ico",
      "target": [
        "zip",
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "uninstallDisplayName": "${productName}"
    },
    "artifactName": "${productName}-${version}-${os}.${ext}",
    "directories": {
      "buildResources": "public",
      "output": "release-builds"
    },
    "asar": "false",
    "extraFiles": [
      {
        "from": "ncvxrl.json",
        "to": "Resources/app",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "repository": "github:sandiz/rs-manager",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
