{
  "name": "rs-manager",
  "description": "Manage Rocksmith playlists, songs, stats and dlc's",
  "author": "sandiz",
  "version": "2.1.9",
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "about-window": "^1.12.1",
    "aes-js": "^3.1.1",
    "album-art": "^2.0.1",
    "await-spawn": "^2.1.1",
    "binary-parser": "^1.3.2",
    "chartjs-plugin-zoom": "^0.6.5",
    "css-toggle-switch": "^4.1.0",
    "csv-parse": "^3.1.2",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "electron-is-dev": "^1.0.1",
    "electron-prompt": "^1.2.0",
    "electron-window-state": "^5.0.1",
    "express": "^4.16.3",
    "find-process": "^1.1.3",
    "highcharts": "^6.2.0",
    "highcharts-react-official": "^2.0.0",
    "line-by-line": "^0.1.6",
    "marked": "^0.6.1",
    "memorystream": "^0.3.1",
    "merge": "^1.2.1",
    "moment": "^2.22.2",
    "naudiodon": "https://github.com/Streampunk/naudiodon",
    "node-abi": "^2.7.0",
    "node-vdf": "^0.0.3",
    "object-assign": "4.1.1",
    "openid": "^2.0.6",
    "pidusage": "^2.0.17",
    "promise": "8.0.2",
    "prop-types": "^15.6.2",
    "psarcjs": "^0.1.4",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-animated-number": "^0.4.4",
    "react-bootstrap-table-next": "^1.2.0",
    "react-bootstrap-table2-filter": "^1.0.0",
    "react-bootstrap-table2-paginator": "^1.0.2",
    "react-collapsible": "^2.3.1",
    "react-datepicker": "^1.6.0",
    "react-dom": "^16.5.2",
    "react-select": "^2.1.2",
    "react-tooltip": "^3.8.4",
    "registry-js": "^1.4.0",
    "request": "^2.34",
    "request-promise": "^4.2.2",
    "shell-env": "^3.0.0",
    "shortid": "^2.2.13",
    "sqlite": "^3.0.0",
    "steam-webapi": "^0.6.5",
    "sudo-prompt": "^8.2.3",
    "sweetalert2": "^7.32.4",
    "tmp": "^0.1.0",
    "wav": "^1.0.2",
    "wavefile": "^8.4.4",
    "webpack-dev-server": "^3.1.1",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "electron": "electron .",
    "electron-start": "node src/electron-wait-react",
    "react-start": "react-app-rewired start",
    "pack": "build --dir",
    "dist-mac": "yarn run build && yarn run package-mac && yarn run dmg-mac",
    "dist-win": "yarn run build && yarn run package-win && yarn run exe-win",
    "postinstall": "electron-rebuild  -f -w sqlite3, naudiodon && patch-package",
    "package-mac": "node scripts/package.js mac",
    "package-win": "node scripts/package.js win",
    "dmg-mac": "mkdir -p node_modules/ds-store/assets && cp src/assets/DSStore-clean node_modules/ds-store/assets/ && electron-installer-dmg ./release-builds/Rocksmith\\ Manager-darwin-x64/Rocksmith\\ Manager.app 'RocksmithManagerInstaller' --out=release-builds --overwrite --icon=src/assets/icons/mac/mac.icns",
    "exe-win": "node scripts/createinstaller.js",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "contributors:check": "all-contributors check"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.0",
    "babel-preset-react-app": "^5.0.3",
    "cross-spawn": "^6.0.5",
    "electron": "^4.0.4",
    "electron-builder": "^20.28.4",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^12.2.0",
    "electron-rebuild": "^1.8.2",
    "electron-winstaller": "^3.0.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "foreman": "^3.0.1",
    "jest": "23.6.0",
    "patch-package": "^5.1.1",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "^2.0.4"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "build": {
    "appId": "com.sandi.rsmanager",
    "win": {
      "icon": "./src/assets/icon.jpg"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "repository": "github:sandiz/rs-manager",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
